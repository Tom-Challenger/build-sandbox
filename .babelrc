{
	"presets": [
		[
			"@babel/preset-env",
			{
				"corejs": 3,
				"useBuiltIns": "entry",
				"debug": true,
				"modules": false,
				"targets": [
					"last 2 chrome versions",
					"last 2 firefox versions",
					"last 2 ios versions",
					"> 0.3%",
					"not ie > 0"
				]
			}
		]
	],

	"plugins": [
		"@babel/plugin-proposal-class-properties"
	]
}